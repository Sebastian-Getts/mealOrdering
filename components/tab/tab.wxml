<view class="tabs">
    <view class="tab_title">
        <view class="tab_item {{item.isActive? 'active':''}}" wx:for="{{tabs}}" wx:key="id" bindtap="handleItemTap" data-index="{{index}}">
            {{item.name}}
        </view>
    </view>
    <view class="tab_content">
        <slot></slot>
    </view>
</view>
